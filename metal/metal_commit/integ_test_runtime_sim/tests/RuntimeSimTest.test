<launch>
  <arg name="rp_dir" value="/tmp/appliance_test" />
  <node pkg="rtr_appliance_app" name="rtr_appliance_app" type="rtr_appliance_app" cwd="node"
    args="--runtime-dir=$(arg rp_dir) --state-dir=$(arg rp_dir) --cache_dir=$(arg rp_dir) --config-dir=$(arg rp_dir) --logs-dir=$(arg rp_dir)/" >
  </node>

  <arg name="rs_dir" value="/tmp/rapidsense_test" />
  <node pkg="rtr_spatial_perception" name="rtr_spatial_perception" type="rtr_spatial_perception" cwd="node"
      args="--runtime-dir=$(arg rs_dir) --state-dir=$(arg rs_dir) --cache_dir=$(arg rs_dir) --config-dir=$(arg rs_dir) --logs-dir=$(arg rs_dir)/">
  </node>

  <test test-name="RuntimeSimTest" pkg="integ_test_runtime_sim" type="RuntimeSimTest" time-limit="180">
    <param name="project" type="string" value="$(find integ_test_runtime_sim)/../../test_data/ur3_runtime_test/ur3_november_25.zip" />
    <param name="rapidsense_data" type="string" value="$(find integ_test_runtime_sim)/../../test_data/ur3_runtime_test/rapidsense_data/" />
    <param name="decon_group_name" type="string" value="ur3_runtime_test"/>
    <param name="robot_name" type="string" value="ur3"/>
    <param name="default_state_space" type="string" value="default_state"/>
    <param name="other_state_space" type="string" value="default_state_2"/>
    <param name="flange_frame" type="string" value="ur3_rtr_flange"/>
    <param name="start_hub_name" type="string" value="point1"/>
    <param name="target_hub_name" type="string" value="point2"/>
  </test>
</launch>
