<launch>
  <arg name="rp_dir" value="/tmp/appliance_test" />
  <node pkg="rtr_appliance_app" name="rtr_appliance_app" type="rtr_appliance_app" cwd="node"
    args="--runtime-dir=$(arg rp_dir) --state-dir=$(arg rp_dir) --cache_dir=$(arg rp_dir) --config-dir=$(arg rp_dir) --logs-dir=$(arg rp_dir)/" >
  </node>

  <arg name="rs_dir" value="/tmp/rapidsense_test" />
  <node pkg="rtr_spatial_perception" name="rtr_spatial_perception" type="rtr_spatial_perception" cwd="node"
      args="--runtime-dir=$(arg rs_dir) --state-dir=$(arg rs_dir) --cache_dir=$(arg rs_dir) --config-dir=$(arg rs_dir) --logs-dir=$(arg rs_dir)/">
  </node>

  
  <test test-name="LatencyTest" pkg="integ_test_latency" type="LatencyTest" time-limit="130">
    <param name="project" type="string" value="$(env RTR_PERCEPTION_TEST_DATA_ROOT)/rapidsense_testdata/ur3_latency_test1/ur3_november_25.zip" />
    <param name="rapidsense_data" type="string" value="$(env RTR_PERCEPTION_TEST_DATA_ROOT)/rapidsense_testdata/ur3_latency_test1/rapidsense_data/" />
    <param name="records" type="string" value="$(env RTR_PERCEPTION_TEST_DATA_ROOT)/rapidsense_testdata/ur3_latency_test1/recordings/" />
    <param name="robot_name"  type="string" value="ur3"/>
  </test>
</launch>
