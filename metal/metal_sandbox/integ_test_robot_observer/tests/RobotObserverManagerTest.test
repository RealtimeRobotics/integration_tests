<launch>
  <arg name="rp_dir" value="/tmp/appliance_test" />
  <node pkg="rtr_appliance_app" name="rtr_appliance_app" type="rtr_appliance_app" cwd="node"
    args="--runtime-dir=$(arg rp_dir) --state-dir=$(arg rp_dir) --cache_dir=$(arg rp_dir) --config-dir=$(arg rp_dir) --logs-dir=$(arg rp_dir)/" >
  </node>

  <arg name="rs_dir" value="/tmp/rapidsense_test" />
  <node pkg="rtr_spatial_perception" name="rtr_spatial_perception" type="rtr_spatial_perception" cwd="node"
      args="--runtime-dir=$(arg rs_dir) --state-dir=$(arg rs_dir) --cache_dir=$(arg rs_dir) --config-dir=$(arg rs_dir) --logs-dir=$(arg rs_dir)/">
  </node>

  <test test-name="RobotObserverManagerTest" pkg="integ_test_robot_observer" type="RobotObserverManagerTest" time-limit="180">
    
    <param name="rapidsense_data" type="string" value="$(find integ_test_robot_observer)/../../test_data/rocket_power/rapidsense_data/" />
    
  </test>
</launch>
